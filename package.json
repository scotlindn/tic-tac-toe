{
  "name": "tic-tac-toe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "PORT=4000 next dev",
    "build": "next build",
    "start": "PORT=4000 next start",
    "lint:format": "prettier --loglevel warn --write \"./**/*.{js,ts,md,json,html,tsx}\"",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --ignore-path=.gitignore --quiet --fix",
    "lint": "run-s lint:format lint:fix",
    "codegen": "graphql-codegen",
    "generate-todos": "leasot -x --reporter markdown 'src/**/*.{ts,tsx}' '*.{js,jsx}' | tee README.md > /dev/null"
  },
  "lint-staged": {
    "src/**/*.(t|j)s?(x)": [
      "eslint  --fix"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.9",
    "@mui/lab": "^5.0.0-alpha.107",
    "@mui/material": "^5.10.13",
    "@next/font": "^13.0.6",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.6",
    "leasot": "^13.2.0",
    "lodash-es": "^4.17.21",
    "next": "13.0.6",
    "react": "18.2.0",
    "react-custom-scrollbars-2": "^4.5.0",
    "react-dom": "18.2.0",
    "react-use": "^17.4.0",
    "typescript": "4.9.4"
  },
  "packageManager": "yarn@3.2.4",
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "husky": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1"
  }
}
